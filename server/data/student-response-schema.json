{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "data": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "age": {
                        "type": "integer"
                    }
                },
                "required": [
                    "id",
                    "name",
                    "age"
                ]
            }
        },
        "total": {
            "type": "integer"
        },
        "query": {
            "type": "string"
        }
    },
    "required": [
        "data",
        "total",
        "query"
    ]
}